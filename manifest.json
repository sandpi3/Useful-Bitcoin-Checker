{
    "manifest_version": 2,
    "name": "Is Bitcoin Rising Again?",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.3.1.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Check if Bitcoin is rising."
    }
}
